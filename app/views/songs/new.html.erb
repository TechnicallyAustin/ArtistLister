<h1>Create a New Song  </h1>
<%=form_for @playlist_song do |t|%>


<%= t.label :name %>
<%= t.text_field :name %>


<%= t.hidden_field :artist_id %>

<%=t.label :rating%>
<%=t.number_field :rating%>

<%=form_for @artist do |k| %>

<%= k.label :name%>
<%= k.text_field :name%>

<%k.submit%>
<%end%>



<%= t.submit%>
<%end%>
