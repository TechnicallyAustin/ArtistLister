<h1> Create a new Playlist </h1>
<%= form_for @playlist do |t| %>
<%= t.label :title %>
<%= t.text_field :title %><br />

    <%= t.fields_for :song do |s| %>

        <%=s.label :song_name %>
        <%=s.text_field :name %>

        <%=s.label :artist_name %>
        <%=s.text_field :artist %><br />

        <%= s.label :rating%>
        <%= s.number_field :rating %>
    <%end%>
    <%= t.submit %>
<%end%>
